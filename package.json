{
	"author": {
		"name": "Oscar Palm√©r",
		"url": "https://oscarpalmer.se"
	},
	"description": "A collection of web components.",
	"devDependencies": {
		"@tsconfig/recommended": "^1.0.1",
		"@types/node": "^17.0.0",
		"@typescript-eslint/eslint-plugin": "^5.27.0",
		"@typescript-eslint/parser": "^5.27.0",
		"esbuild": "^0.14.0",
		"eslint": "^8.17.0",
		"eslint-config-xo": "^0.41.0",
		"eslint-config-xo-typescript": "^0.51.0",
		"typescript": "^4.7.0"
	},
	"eslintConfig": {
		"extends": [
			"xo",
			"xo-typescript/space"
		]
	},
	"exports": {
		".": {
			"import": "./dist/index.js",
			"require": "./dist/index.js"
		}
	},
	"jsdelivr": "dist/index.js",
	"keywords": [
		"component",
		"components",
		"frontend",
		"progressive enhancement",
		"web component",
		"web components"
	],
	"license": "MIT",
	"main": "dist/index.js",
	"module": "dist/index.js",
	"name": "@oscarpalmer/components",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/oscarpalmer/components.git"
	},
	"scripts": {
		"build": "zsh -c 'esbuild ./src/**/*.ts --bundle --target=es2020 --platform=browser --outdir=dist --format=esm'"
	},
	"unpkg": "dist/index.js",
	"version": "0.1.0"
}
